{{>layout/header}}
    <link rel="stylesheet" href="/css/detail-style.css">
    <section class="container">
        <div class="content__title__box">
            <div class="content__title__1">{{model.common.title}}</div>
            <div class="content__title__2">{{model.common.addr1}}</div>
